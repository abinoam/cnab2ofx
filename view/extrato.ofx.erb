OFXHEADER:100
DATA:OFXSGML
VERSION:102
SECURITY:NONE
ENCODING:USASCII
CHARSET:1252
COMPRESSION:NONE
OLDFILEUID:NONE
NEWFILEUID:NONE

<OFX>
    <SIGNONMSGSRSV1>
        <SONRS>
            <STATUS>
                <CODE>0
                <SEVERITY>INFO
            </STATUS>
            <DTSERVER><%=@dtserver%>
            <LANGUAGE>POR
            <FI>
                <ORG><%=@org%>
                <FID><%=@fid%>
            </FI>
        </SONRS>
    </SIGNONMSGSRSV1>
    <BANKMSGSRSV1>
        <STMTTRNRS>
            <TRNUID>1
            <STATUS>
                <CODE>0
                <SEVERITY>INFO
            </STATUS>
            <STMTRS>
                <CURDEF>BRL
                <BANKACCTFROM>
                    <BANKID><%=@bankid%>
                    <BRANCHID><%=@branchid%>
                    <ACCTID><%=@acctid%>
                    <ACCTTYPE>CHECKING
                </BANKACCTFROM>
                <BANKTRANLIST>
                    <DTSTART><%=@dtserver%>
                    <DTEND><%=@dtserver%>
                  <% @transactions.each do |t| %>
                    <STMTTRN>
                        <TRNTYPE>OTHER
                        <DTPOSTED><%=t[:dtposted]%>
                        <TRNAMT><%=t[:trnamt]%>
                        <FITID><%=t[:fitid] %>
                        <CHECKNUM><%=t[:checknum] %>
                        <PAYEEID>0
                        <MEMO><%=t[:memo]%>
                    </STMTTRN>
                  <% end %>
                </BANKTRANLIST>
                <LEDGERBAL>
                    <BALAMT><%=@balamt%>
                    <DTASOF><%=@dtasof%>
                </LEDGERBAL>
            </STMTRS>
        </STMTTRNRS>
    </BANKMSGSRSV1>
</OFX>